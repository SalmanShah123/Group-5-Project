import pandas as pd
import camelot
import ghostscript

pdf_path = "/Users/alanlin/Desktop/Project /Data/October-1-2023-to-october-7-2023.pdf"

tables = camelot.read_pdf(pdf_path, pages='all') #take a while to run the code 
tables # checking the number of pages 

all_df = []
for i in range(tables.n):
    df = tables[i].df
    all_df.append(df)

concatenated_df = []
for df in all_df:
    concatenated_df.append(df)

final_df = pd.concat(concatenated_df, axis=0)

path = "/Users/alanlin/Nov26-DEC02.csv"  #change it to your own file path
final_df.to_csv(path, index = False)